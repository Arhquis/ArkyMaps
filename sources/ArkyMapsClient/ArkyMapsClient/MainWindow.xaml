<Window x:Class="ArkyMapsClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:ArkyMapsClient.Views"
        Title="Arky maps" Height="720" Width="1280" WindowStartupLocation="CenterScreen">
    <TabControl>
        <TabItem Header="Login">
            <StackPanel>
                <Button Content="Connect" Click="Button_Click" />
                <TextBox Text="semmi" x:Name="m_textBox"/>
                <Button Content="Disconnect" Click="Button_Click_1" />
                <ProgressBar Height="10"/>
            </StackPanel>
        </TabItem>
        <TabItem Header="Map">
            <Grid>
                <views:RealTimeView x:Name="m_realTimeView" />
            </Grid>
        </TabItem>
        <TabItem Header="Administration">

        </TabItem>
    </TabControl>
</Window>
